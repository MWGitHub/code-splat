<div id="content">
	<script type="text/javascript">
		var codeSplat = {
			codeLanguages: [],
			codelanguageProperties: {
				javascript: "<%= asset_path('language_icons/javascript.png') %>",
				ruby: "<%= asset_path('language_icons/ruby.png') %>"
			}
		};
		window.codeSplat = codeSplat;
		<% Project::CODE_LANGUAGES.each do |language| %>
			codeSplat.codeLanguages.push(<%= "\"#{language}\"".html_safe %>);
		<% end %>
		codeSplat.homeIconPath = "<%= asset_path('home-code-icon.png') %>";
	</script>
  <h1>CodeSplat</h1>
  <p>Loading...</p>
  <!-- <p>Sorry, your browser is not supported.</p> -->
</div>
